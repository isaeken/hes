{
    "name": "isaeken/hes",
    "version": "1.0",
    "type": "library",
    "description": "PHP Hayat Eve Sığar API",
    "keywords": [
        "api",
        "hayat eve sığar",
        "hes",
        "korona",
        "corona",
        "corona virus",
        "covid19"
    ],
    "homepage": "https://github.com/isaeken/hes",
    "license": "MIT",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "readme": "README.md",
    "authors": [
        {
            "name": "Isa Eken",
            "email": "hello@isaeken.com.tr",
            "homepage": "https://www.isaeken.com.tr",
            "role": "Founder"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "./vendor/bin/phpunit --coverage-html coverage"
    },
    "autoload": {
        "psr-4": {
            "IsaEken\\Hes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IsaEken\\Hes\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.3",
        "illuminate/support": "^8.58",
        "isaeken/thinks": "^1.0",
        "nesbot/carbon": "^2.52"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "orchestra/testbench": "^6.21",
        "pestphp/pest": "^1.20",
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.3",
        "vimeo/psalm": "^4.10"
    }
}
